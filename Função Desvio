#include <stdio.h>
#include <math.h>
#define N 4
void main()
{
    int i;
    float v[]= {2, 2, 3, 3}, media=0, desvio=0;

    for (i=0; i<N; i++)
        {
            media+=v[i];
        }
    media=media/N;
    for (i=0; i<N; i++)
    {
        desvio+=pow((v[i]-media), 2);
    }
    desvio=sqrt(desvio/(N-1));
    printf("%f", desvio);
}
